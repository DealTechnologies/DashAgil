{
  "ConnectionString": {
    "Connection": "Data Source={{server}};Initial Catalog={{database}};User ID={{user}};Password={{pass}};Integrated Security=False;"
  },
  "AwsSettings": {
    "KeyId": "{{aws-keyId}}",
    "SecretKey": "{{aws-secret-key}}",
    "ConfigurationSetsName": "{{aws-ses-configurationName}}",
    "FromAddress": "{{from-address}}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
